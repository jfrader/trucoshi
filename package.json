{
  "name": "trucoshi",
  "version": "0.5.10",
  "main": "dist/types.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "nodemon",
    "profiler": "yarn build && node --prof ./bin/trucoshi-server",
    "build": "yarn run rimraf ./dist && yarn run tsc --project .",
    "prepublishOnly": "yarn test && yarn build",
    "test": "yarn run ts-mocha --exit ./test/**/*.ts",
    "test:autoplay": "yarn run ts-node src/test/autoplay",
    "test:play": "yarn run ts-node src/test/play"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "socket.io-client": "^4.5.4",
    "ts-mocha": "^10.0.0"
  },
  "files": [
    "dist/lib/constants.js",
    "dist/lib/constants.d.ts",
    "dist/types.js",
    "dist/types.d.ts",
    "dist/server/constants.js",
    "dist/server/constants.d.ts"
  ],
  "dependencies": {
    "@types/pino": "^7.0.5",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "rimraf": "^4.1.1",
    "socket.io": "^4.5.4",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  }
}