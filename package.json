    "ts-mocha": "^10.0.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/types.js",
    "dist/types.d.ts",
    "dist/lib/**/*"
  ],
  "dependencies": {
    "rimraf": "^4.1.1",
    "socket.io": "^4.5.4",
